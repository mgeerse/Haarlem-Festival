<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Haarlem Festival</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <link rel="stylesheet" href="../Content/NavigationBar.css" />
</head>
<body>
    <nav class="navbar navbar-inverse">
        <div class="container-fluid">
            <ul class="nav navbar-nav navbar-static-top">
                <li><a href="#">Home</a></li>
                <li><a href="#">Historic Tours</a></li>
                <li><a href="#">Dining in Haarlem</a></li>
                <li><a href="#">Jazz@Patronaat</a></li>
                <li><a href="#">Talking Haarlem</a></li>
            </ul>
        </div>
    </nav>
    <div class="container body-content">
        @RenderBody()

        @*<footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>*@
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>